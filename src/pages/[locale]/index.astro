---
import HomePage from "@/components/pages/home-page.astro";
import { getLocaleFromParams, getLocaleStaticPaths } from "@/i18n/utils";

export function getStaticPaths() {
  return getLocaleStaticPaths();
}

export const prerender = true;

const locale = getLocaleFromParams(Astro.params.locale);
---

<HomePage locale={locale} />
